<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="JetBrains.Icons.Avalonia.Demo.App"
             xmlns:local="using:JetBrains.Icons.Avalonia.Demo"
             xmlns:avalonia="clr-namespace:JetBrains.Icons.Avalonia;assembly=JetBrains.Icons.Avalonia"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <avalonia:JetBrainsIconStyles />
    </Application.Styles>
</Application>