<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:JetBrains.Icons.Avalonia.Demo.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:avalonia="clr-namespace:JetBrains.Icons.Avalonia;assembly=JetBrains.Icons.Avalonia"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="300"
        x:Class="JetBrains.Icons.Avalonia.Demo.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Background="DarkSlateGray"
        Icon="/Assets/avalonia-logo.ico"
        Title="JetBrains.Icons.Avalonia.Demo">

    <Design.DataContext>
        <!-- This only sets the DataContext for the previewer in an IDE,
             to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs) -->
        <vm:MainWindowViewModel/>
    </Design.DataContext>
    <!--  <TextBlock Text="{Binding Greeting}" HorizontalAlignment="Center" VerticalAlignment="Center"/> -->
     <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
         <avalonia:JetBrainsIcon Kind="SuccessDialogDark" Width="32" Height="32" /> 
    </StackPanel>
   
    
</Window>
